  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "@octokit/graphql": "^7.0.2",
    "@octokit/rest": "^20.0.2",
    "azure-devops-node-api": "^12.5.0",
    "showdown": "^2.1.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ni/ni-github-actions-issue-to-work-item"
  }
