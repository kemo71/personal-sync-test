{
  "name": "github-to-azure-devops-sync",
  "version": "2.0.0",
  "description": "Enhanced GitHub Issues to Azure DevOps Work Items synchronization with bulk migration support",
  "main": "index-enhanced.js",
  "scripts": {
    "start": "node index-enhanced.js",
    "test": "node index-enhanced.js",
    "migrate-all": "MIGRATION_MODE=bulk_all node index-enhanced.js",
    "migrate-open": "MIGRATION_MODE=bulk_open node index-enhanced.js",
    "migrate-closed": "MIGRATION_MODE=bulk_closed node index-enhanced.js"
  },
  "keywords": [
    "github",
    "azure-devops",
